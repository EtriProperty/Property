<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>메인페이지</title>

    <!-- <% %> -->
    <!-- Favicon -->
    <!-- <link rel="shortcut icon" href="img/favicon.ico" type="image/x-icon"> -->

    <!-- Font awesome -->
    <!-- <link href="css/font-awesome.css" rel="stylesheet"> -->
    <!-- Bootstrap -->
    <link href="css/bootstrap.css" rel="stylesheet" />
    <!-- slick slider -->
    <link rel="stylesheet" type="text/css" href="css/slick.css" />
    <!-- price picker slider -->
    <!-- <link rel="stylesheet" type="text/css" href="css/nouislider.css"> -->
    <!-- Fancybox slider -->
    <link
      rel="stylesheet"
      href="css/jquery.fancybox.css"
      type="text/css"
      media="screen"
    />
    <!-- Theme color -->
    <link
      id="switcher"
      href="css/theme-color/default-theme.css"
      rel="stylesheet"
    />

    <!-- Main style sheet -->
    <link href="css/style.css" rel="stylesheet" />

    <!-- Google Font -->
    <!-- <link href='https://fonts.googleapis.com/css?family=Vollkorn' rel='stylesheet' type='text/css'>     -->
    <!-- <link href='https://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css'> -->

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>
  <body class="aa-price-range">
    <!-- Pre Loader -->
    <div id="aa-preloader-area">
      <div class="pulse"></div>
    </div>
    <!-- SCROLL TOP BUTTON -->
    <a class="scrollToTop" href="#"><i class="fa fa-angle-double-up"></i></a>
    <!-- END SCROLL TOP BUTTON -->

    <!-- Start header section -->
    <header id="aa-header">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <div class="aa-header-area">
              <div class="row">
                <div class="col-md-6 col-sm-6 col-xs-6"></div>
                <div class="col-md-6 col-sm-6 col-xs-6">
                  <div class="aa-header-right">
                    <a href="/login" class="aa-login"
                      ><% if(islogined){ %>
                      <a href="/mywishlist" class="aa-register"
                        ><%= userid %></a
                      >
                      <a href="/logout" class="aa-login">로그아웃</a>
                      <% } else { %>
                      <a href="/register" class="aa-register">회원가입</a>
                      <a href="/login" class="aa-login">로그인</a>
                      <% } %></a
                    >
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </header>
    <!-- End header section -->
    <!-- Start menu section -->
    <section id="aa-menu-area">
      <nav class="navbar navbar-default main-navbar" role="navigation">
        <div class="container">
          <div class="navbar-header">
            <!-- FOR MOBILE VIEW COLLAPSED BUTTON -->
            <!-- LOGO -->
            <!-- Text based logo -->
            <a href="/"><img src="img/logo.jpg" width="150" height="150"/></a>
            <!-- Image based logo -->
            <!-- <a class="navbar-brand aa-logo-img" href="indexl"><img src="img/logo.png" alt="logo"></a> -->
          </div>
          <div id="navbar" class="navbar-collapse collapse">
            <ul id="top-menu" class="nav navbar-nav navbar-right aa-main-nav">
              <li><a href="/properties">매물찾기</a></li>
              <li>
                <a href="/properties-register">매물등록</a>
              </li>
              <li>
                <a href="/mywishlist">마이페이지</a>
              </li>
            </ul>
          </div>
          <!--/.nav-collapse -->
        </div>
      </nav>
    </section>
    <!-- End menu section -->

    <!-- Start Proerty header  -->

    <section id="aa-property-header">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <div class="aa-property-header-inner">
              <h2>중개자 없이 방을 구하세요</h2>
              <a>메타 마스크가 필요합니다.</a>
            </div>
          </div>
        </div>
      </div>
    </section>
    <!-- End Proerty header  -->

    <!-- Start Properties  -->
    <section id="aa-properties">
      <div class="container">
        <div class="row">
          <div class="col-md-8">
            <div class="aa-properties-content">
              <!-- start properties content head -->
              <div class="aa-properties-content-head">
                <span>최근 매물</span>
                <div class="aa-properties-content-head-right">
                  <a id="aa-grid-properties" href="#"
                    ><span class="fa fa-th"></span
                  ></a>
                  <a id="aa-list-properties" href="#"
                    ><span class="fa fa-list"></span
                  ></a>
                </div>
              </div>
              <!-- Start properties content body -->
              <form name="listform" action="/recentList" method="get">
                <div class="aa-properties-content-body">
                  <!--감싼부분-->
                  <% arrays.forEach(data => { %>
                  <ul class="aa-properties-nav">
                    <li>
                      <article class="aa-properties-item">
                        <a class="aa-properties-item-img" href="#">
                          <img
                            style="width:360px; height:220px;"
                            alt="img"
                            src="<%=data['img']%>"
                          />
                        </a>
                        <div class="aa-tag for-rent">
                          <%= data['roomtype'] %>
                        </div>
                        <div class="aa-properties-item-content">
                          <div class="aa-properties-about">
                            <h3>
                              <a
                                href="/properties-detail/<%= data['id'] %>"
                              ></a>
                            </h3>
                            <p>
                              매물번호: <%= data['id'] %> <br />
                              주소: <%= data['address'] %> <br />
                              집주인 연락처: <%= data['phonenumber'] %> <br />
                              매물형태: <%= data['roomtype'] %> <br />
                              방개수: <%= data['roomcount'] %> <br />
                              방평수: <%= data['roomsize'] %> <br />
                              게시일: <%= data['date'] %> <br />
                            </p>
                          </div>
                          <div class="aa-properties-detial">
                            <span class="aa-price">
                              가격: <%= data['price'] %>ETH
                            </span>
                            <a
                              class="aa-secondary-btn"
                              href="/properties-detail/<%= data['id'] %>"
                              >자세히 보기</a
                            >
                          </div>
                        </div>
                      </article>
                    </li>
                  </ul>
                  <% }) %>
                </div>
              </form>
            </div>
          </div>

          <!-- Start properties sidebar -->
          <div class="col-md-4">
            <aside class="aa-properties-sidebar">
              <!-- Start Single properties sidebar -->
              <div class="aa-properties-single-sidebar">
                <h3>조건 검색</h3>
                <form name="searchform" action="/" method="post">
                  <div class="aa-single-advance-search">
                    <select id="" name="">
                      <option selected="" value="0">지역구</option>
                      <option value="1">관악구</option>
                      <option value="2">서초구</option>
                      <option value="3">구로구</option>
                      <option value="4">모름구</option>
                    </select>
                  </div>

                  <div class="aa-single-advance-search">
                    <input type="submit" value="Search" class="aa-search-btn" />
                  </div>
                </form>
              </div>
              <!-- Start Single properties sidebar -->
            </aside>
          </div>
        </div>
      </div>
    </section>
    <!-- / Properties  -->

    <!-- Footer -->
    <footer></footer>
    <!-- / Footer -->

    <!-- jQuery library -->
    <!--<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script> -->
    <script src="js/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="js/bootstrap.js"></script>
    <!-- slick slider -->
    <script type="text/javascript" src="js/slick.js"></script>
    <!-- Price picker slider -->
    <!-- <script type="text/javascript" src="js/nouislider.js"></script> -->
    <!-- mixit slider -->
    <script type="text/javascript" src="js/jquery.mixitup.js"></script>
    <!-- Add fancyBox -->
    <script type="text/javascript" src="js/jquery.fancybox.pack.js"></script>
    <!-- Custom js -->
    <script src="js/custom.js"></script>
  </body>
</html>
