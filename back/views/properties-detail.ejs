<!-- 매물정보도 보여줘야하고 data(number,roomtype,address,roomsiez,roomcount,price,phonenumber) 객체 뿌려주긴함, 이미지도 불러줘야함-->
<!-- 후기도 보여줘야함 이건 array(content,user,rtime) 라고 뿌려줄거임-->
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>디방 중개자없이 방구하자!</title>

    <!-- Favicon -->
    <link rel="shortcut icon" href="../img/favicon.ico" type="image/x-icon" />

    <!-- Font awesome -->
    <link href="../css/font-awesome.css" rel="stylesheet" />
    <!-- Bootstrap -->
    <link href="../css/bootstrap.css" rel="stylesheet" />
    <!-- slick slider -->
    <link rel="stylesheet" type="text/css" href="../css/slick.css" />
    <!-- price picker slider -->
    <link rel="stylesheet" type="text/css" href="../css/nouislider.css" />
    <!-- Fancybox slider -->
    <link
      rel="stylesheet"
      href="../css/jquery.fancybox.css"
      type="text/css"
      media="screen"
    />
    <!-- Theme color -->
    <link
      id="switcher"
      href="../css/theme-color/default-theme.css"
      rel="stylesheet"
    />

    <!-- Main style sheet -->
    <link href="../css/style.css" rel="stylesheet" />

    <!-- Google Font -->
    <link
      href="https://fonts.googleapis.com/css?family=Vollkorn"
      rel="stylesheet"
      type="text/css"
    />
    <link
      href="https://fonts.googleapis.com/css?family=Open+Sans"
      rel="stylesheet"
      type="text/css"
    />

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>
  <body class="aa-price-range">
    <!-- Pre Loader -->
    <div id="aa-preloader-area">
      <div class="pulse"></div>
    </div>
    <!-- SCROLL TOP BUTTON -->
    <a class="scrollToTop" href="#"><i class="fa fa-angle-double-up"></i></a>
    <!-- END SCROLL TOP BUTTON -->

    <!-- Start header section -->
    <header id="aa-header">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <div class="aa-header-area">
              <div class="row">
                <div class="col-md-6 col-sm-6 col-xs-6"></div>
                <div class="col-md-6 col-sm-6 col-xs-6">
                  <div class="aa-header-right">
                    <a href="/login" class="aa-login"
                      ><% if(islogined){ %>
                      <a href="/mywishlist" class="aa-register"
                        ><%= userid %></a
                      >
                      <a href="/logout" class="aa-login">로그아웃</a>
                      <% } else { %>
                      <a href="/register" class="aa-register">회원가입</a>
                      <a href="/login" class="aa-login">로그인</a>
                      <% } %></a
                    >
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </header>

    <!-- End header section -->
    <!-- Start menu section -->
    <section id="aa-menu-area">
      <nav class="navbar navbar-default main-navbar" role="navigation">
        <div class="container">
          <div class="navbar-header">
            <!-- FOR MOBILE VIEW COLLAPSED BUTTON -->
            <!-- LOGO -->
            <!-- Text based logo -->
            <a href="/"
              ><img src="../img/logo.jpg" width="120" height="120"
            /></a>
            <!-- Image based logo -->
            <!-- <a class="navbar-brand aa-logo-img" href="index"><img src="img/logo.png" alt="logo"></a> -->
          </div>
          <div id="navbar" class="navbar-collapse collapse">
            <ul id="top-menu" class="nav navbar-nav navbar-right aa-main-nav">
              <li><a href="/properties">매물찾기</a></li>
              <li>
                <a href="/properties-register" onclick="showConfirm();"
                  >매물등록</a
                >
              </li>
              <li>
                <a href="/mywishlist" onclick="showConfirm();">마이페이지</a>
              </li>
            </ul>
          </div>
          <!--/.nav-collapse -->
        </div>
      </nav>
    </section>
    <!-- End menu section -->

    <!-- Start Proerty header  -->

    <section id="aa-property-header">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <div class="aa-property-header-inner">
              <h2>매물 정보</h2>
            </div>
          </div>
        </div>
      </div>
    </section>
    <!-- End Proerty header  -->
    <!-- Start Properties  -->
    <section id="aa-properties">
      <div class="container">
        <div class="row">
          <div class="col-md-8">
            <div class="aa-properties-content">
              <!-- Start properties content body -->
              <div class="aa-properties-details">
                <div class="properties">
                  <hr />
                  <h2>조용한 주택가 입니다.</h2>
                  <hr />
                  <div class="wave">
                    <a
                      href="/mywishlist/addmywishlist"
                      onclick="return addwish();"
                      >찜하기
                    </a>
                  </div>
                  &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                  <div class="wave2">
                    <a data-toggle="modal" href="#myModal">계약신청</a>
                  </div>
                  <hr />
                  <h3>상세 정보</h3>
                  <ul>
                    <li>매물번호:<span id="contentNumber"><%= data['number'] %></span></li>
                    <li>매물형태:<span><%= data['roomtype'] %></span></li>
                    <li>   주소:<span><%= data['address'] %></span></li>
                    <li>   평수:<span><%= data['roomsize'] %></span></li>
                    <li>방개수: <span><%= data['roomcount'] %></span></li>
                    <li>   가격:<span id="contentPrice"> <%= data['price'] %></span>ETH</li>
                    <li>   옵션 :<span> <%= data['text'] %></span></li>
                    <input type="hidden" id="ethaccount" value="<%= data['owneraccount'] %>"></input>
                    <input type="hidden" id="userEthAcc" value="<%= useracc %>"></input>

                  </ul>
                  <hr />
                  <h3>지도</h3>
                  <br />
                  <iframe
                    src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d6851.201919469417!2d-86.11773906635584!3d33.47324776828677!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x888bdb60cc49c571%3A0x40451ca6baf275c7!2s36008+AL-77%2C+Talladega%2C+AL+35160%2C+USA!5e0!3m2!1sbn!2sbd!4v1460452919256"
                    width="100%"
                    height="450"
                    frameborder="0"
                    style="border:0"
                    allowfullscreen
                  ></iframe>
                  <hr />
                </div>
              </div>
              <main3>
                <div class="main-box3">
                  <br />
                  <form
                    name="review-form"
                    action="/properties-detail/reply/<%= data['number'] %>"
                    method="post"
                  >
                    <br /><br />
                    <td><h3>후기를 적어주세요.</h3></td>
                    <td></td>
                    <textarea
                      id="review-text"
                      cols="10"
                      style="width:600px;height:100px;"
                      placeholder=" 방을 본 후기를 적어주세요 "
                      name="content"
                      class="form-control"
                    ></textarea>
                    <br />
                    <div class="reviewbtn">
                      <input
                        type="submit"
                        value="등록하기"
                        onclick="refresh();"
                      />
                    </div>
                  </form>
                  <form
                    name="review-form"
                    action="/homepage/replydelete"
                    method="delete"
                  >
                    <br />
                    <br />
                    <br />

                    <table class="comment">
                      <% array.forEach(data => { %>
                      <tr>
                        <th scope="row"><%= data['user'] %></th>
                        <td>
                          <%= data['content'] %>
                        </td>
                        <td>
                          <%= data['rtime'] %>
                        </td>
                      </tr>
                      <% }) %>
                    </table>
                  </form>
                  <br />
                  <br />
                </div>
              </main3>
            </div>
          </div>
        </div>
      </div>
    </section>

    <!-- modal  -->
     <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
      <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
            <h4 class="modal-title" id="myModalLabel">정말로 계약하시겠습니까?</h4>
          </div>
          <div class="modal-body">
            소량의 이더가 필요합니다
          </div>
          <div class="modal-footer">
         
            <button type="button" class="btn btn-default" data-dismiss="modal">닫기</button>
            <button type="submit" id="conBtn" onclick="addRoom()" class="btn btn-primary">계약하기</button>
          
          </div>
        </div>
      </div>
    </div>  
    
    <!-- jQuery library -->
    <!--<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script> -->
    <script src="../js/jquery.min.js"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="../js/bootstrap.js"></script>
    <!-- slick slider -->
    <script type="text/javascript" src="../js/slick.js"></script>
    <!-- Price picker slider -->
    <script type="text/javascript" src="../js/nouislider.js"></script>
    <!-- mixit slider -->
    <script type="text/javascript" src="../js/jquery.mixitup.js"></script>
    <!-- Add fancyBox -->
    <script type="text/javascript" src="../js/jquery.fancybox.pack.js"></script>
    <!-- Custom js -->
    <script src="../js/custom.js"></script>
    <script src="../js/dapp.js"></script>
    <script language="javascript">
      function refresh() {
        setTimeout(function() {
          location.reload();
        }, 1000);
      }

      function addwish() {
        return confirm("추가하시겠습니까?");
      }
      
    </script>
   
  </body>
</html>
